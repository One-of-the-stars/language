#include<stdio.h>
#define NAME"Xue"
#define SURNAME"Wei"
int main(void) {

	printf("%s %s\n", NAME, SURNAME);
	printf("%s\n%s\n", NAME, SURNAME);
	printf("%s", NAME);
	printf("%s\n", SURNAME);
	return 0;
}

#include<stdio.h>
constexpr auto NAME = "Xue";
constexpr auto SURNAME = "Wei";
int main(void) {

	printf("%s %s\n", NAME, SURNAME);
	printf("%s\n%s\n", NAME, SURNAME);
	printf("%s", NAME);
	printf("%s\n", SURNAME);
	return 0;
}
